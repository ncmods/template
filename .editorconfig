root = true

[*]
charset                  = utf-8
end_of_line              = lf
insert_final_newline     = true
tab_width                = 4
trim_trailing_whitespace = true

[*.gradle]
indent_style = tab

[*.java]
indent_style = tab

[*.json]
indent_size  = 2
indent_style = space

[*.properties]
indent_size  = 2
indent_style = space

[.editorconfig]
indent_size  = 4
indent_style = space
